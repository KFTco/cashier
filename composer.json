{
  "name": "vandarpay/cashier",
  "description": "Official Vander package for easier access to Vander documents",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vandar",
      "email": "info@vandar.io"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.2.5|^8.0|^8.1",
    "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
    "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0",
    "illuminate/routing": "^6.0|^7.0|^8.0|^9.0|^10.0",
    "illuminate/http": "^6.0|^7.0|^8.0|^9.0|^10.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4|^9.3.3|^10.0",
    "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Vandar\\Cashier\\": "src/",
      "Vandar\\Cashier\\Database\\Factories\\": "database/factories/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vandar\\Cashier\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vandar\\Cashier\\VandarCashierServiceProvider"
      ]
    }
  }
}
